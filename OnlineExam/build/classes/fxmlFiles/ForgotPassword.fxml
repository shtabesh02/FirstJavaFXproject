<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="279.0" prefWidth="503.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.ForgotPasswordController">
   <children>
      <Label layoutX="16.0" layoutY="41.0" text="Fill out the following form to get a copy of your password." wrapText="true">
         <font>
            <Font name="Century Gothic" size="17.0" />
         </font>
      </Label>
      <Label layoutX="123.0" layoutY="115.0" text="Username:" />
      <Label layoutX="123.0" layoutY="156.0" text="Email:" />
      <TextField fx:id="username" layoutX="204.0" layoutY="111.0" />
      <TextField fx:id="email" layoutX="204.0" layoutY="152.0" />
      <Button layoutX="226.0" layoutY="212.0" mnemonicParsing="false" onAction="#submit" text="Submit" />
      <Button layoutX="301.0" layoutY="212.0" mnemonicParsing="false" text="Cancel" />
   </children>
</AnchorPane>
